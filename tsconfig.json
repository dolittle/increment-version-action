{
    "extends": "@dolittle/typescript.config",
    "compilerOptions": {
        "target": "es2015",
        "moduleResolution": "node",
        "composite": false,
        "declaration": false,
        "declarationMap": false,
        "sourceMap": false,
        "allowSyntheticDefaultImports": true,
        "outDir": "Distribution"
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
}
